<!DOCTYPE html>
<html>
	<head>
		<title>registration</title>
	</head>
		<body>
		<h2>Registration page</h2>
			<form action="adminregistration" method="post">
				<table>
				   <tr>
						<th>firstName</th>
							<td><input type="firstName" name="firstName"></td>
					</tr>
					<tr>
						<th>lastName</th>
							<td><input type="lastName" name="lastName"></td>
					</tr>
				
					<tr>
						<th>emailId</th>
							<td><input type="email" name="emailId"></td>
					</tr>
					
					<tr>
						<th>password</th>
							<td><input type="password" name="password"></td>
					</tr>
					<tr>
						<th>mobileNumber</th>
							<td><input type="mobileNumber" name="mobileNumber"></td>
					</tr>
					<tr>
						<th>createdOn</th>
							<td><input type="createdOn" name="createdOn"></td>
					</tr>
					<tr>
						<th>updatedOn</th>
							<td><input type="updatedOn" name="updatedOn"></td>
					</tr>
					
					<tr>
					     <td colspan="2"><input type="submit" value="register"></td>
					</tr>
					
				
				</table>
				
			</form>
		
		
</body>
</html>